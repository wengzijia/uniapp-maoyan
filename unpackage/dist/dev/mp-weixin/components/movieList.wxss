.movieListContainer{
  display: flex;
  justify-content: space-between;
  padding: 40rpx;
}
.movieListContainer .imgWrap {
    width: 128rpx;
    height: 180rpx;
}
.movieListContainer .imgWrap image{
  width: 128rpx;
  height: 180rpx;
}
.movieListContainer .content{
  font-size: 22rpx;
  width: 360rpx;
}
.movieListContainer .content .titleWrap {
  display: flex;
}
.movieListContainer .content .titleWrap .title{
  font-weight: 700;
  font-size: 28rpx;
}
.movieListContainer .content .titleWrap  .max2d {
  margin-left: 20rpx;
  display: inline-block;
  width: 100rpx;
  height: 40rpx;
  background-image: url(data:image/png;base64,);
  background-size: contain;
}
.mgTop{
  margin-top: 10rpx;
}
.movieListContainer .content .score{
  color: #faaf00;
}
.movieListContainer  .ticket{
  width: 140rpx;
  margin-top: 60rpx;
  font-size: 22rpx;
  color: white;
  background-color: red;
  border-radius: 40rpx;
}
.movieListContainer  .presell{
  width: 140rpx;
  margin-top: 60rpx;
  font-size: 22rpx;
  color: white;
  background-color: #3c9fe6;
  border-radius: 40rpx;
}
.movieListContainer  .wannaSee{
  width: 140rpx;
  margin-top: 60rpx;
  font-size: 22rpx;
  color: white;
  background-color: #faaf00;
  border-radius: 40rpx;
}
.movieListContainer  .preview{
  width: 140rpx;
